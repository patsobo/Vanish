//Gravity
vel_x+=gravity_x;
vel_y+=gravity_y;

//Dampening
if(abs(vel_x) &lt; 1)
    vel_x=0;
vel_x*=dampening;
//if on ground apply friction
if( place_meeting(x,y+1,obj_wall))
    vel_x*=frict;
vel_y*=dampening;



//Update position
if(place_meeting(x+vel_x,y,obj_wall))
{
    while(!place_meeting(x+sign(vel_x),y,obj_wall))
    {
        x += sign(vel_x);
    }
    vel_x = 0;
}
x+=vel_x / 2;


if(place_meeting(x,y+vel_y,obj_wall))
{
    while(!place_meeting(x,y+sign(vel_y),obj_wall))
    {
        y += sign(vel_y);
    }
    vel_y = 0;
}
y+=vel_y;

//lightning
rain_counter -= 1;

show_debug_message(rain_counter);
    if (obj_player.rain_counter &lt; 0){
