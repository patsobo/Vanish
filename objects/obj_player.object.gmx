    {
        var del_x = mouse_x-x;
        var del_y = mouse_y-y;
        var x_percent =abs(del_x)/(abs(del_x)+abs(del_y));
        vel_x=other.vel_x+sign(del_x)*abs(x_percent*other.throw_speed);
        vel_y=other.vel_y+sign(del_y)*abs((1-x_percent)*other.throw_speed);
        sprite_index=spr_right_portal;
    }
}




